{
  "name": "eps-sure-next",
  "private": true,
  "version": "1.0.0",
  "description": "Sure - EPS Analytics Data Explorer",
  "license": "UNLICENCED",
  "main": "src/index.js",
  "author": "nobody <nobody@nowhere.com>",
  "engines": {
    "node": ">8.0.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "data",
    "explore",
    "anomaly"
  ],
  "repository": {
    "type": "git",
    "url": "https://nevermind.com/whocares.git"
  },
  "scripts": {
    "pretest": "yarn run coffee:compile",
    "prestart": "yarn run coffee:compile",
    "test": "jest",
    "start": "npm-run-all --parallel start:dev",
    "build": "./node_modules/.bin/babel-node ./webpack/build.js",
    "start:dev": "./node_modules/webpack-dashboard/bin/webpack-dashboard.js -- ./node_modules/.bin/babel-node ./webpack/dev_server",
    "start:dist": "./node_modules/.bin/babel-node ./webpack/dist_server.js",
    "lint": "npm-run-all lint:js lint:css",
    "lint:js": "node_modules/.bin/eslint --config .eslintrc src tests",
    "lint:css": "./node_modules/stylelint/bin/stylelint.js --config=.stylelintrc ./src/styles/**/*.css",
    "lint:flow": "./node_modules/.bin/flow check",
    "clean": "rm -rf ./dist && mkdir dist",
    "coverage:type": "./node_modules/flow-coverage-report/bin/flow-coverage-report.js -o ./dist/flow-coverage-report -i './src/**/*.js' -i './test/unit/**/*.js' -t html -t text --threshold 50",
    "audit": "./node_modules/auditjs/audit.js --package package.json",
    "generate:css": "./node_modules/postcss-cli/bin/postcss src/styles/base.css --config webpack/postcss.config.js --output dist/styles.css",
    "generate:styleguide": "npm-run-all generate:css && ./node_modules/.bin/babel-node scripts/livingcss.js",
    "postinstall": "rm -rf flow-typed && ./node_modules/flow-typed/dist/cli.js install && ./node_modules/flow-scripts/bin/flow-scripts.js stub",
    "test:watch": "jest --watch",
    "restartFlowServer": "./node_modules/.bin/flow stop && ./node_modules/.bin/flow start",
    "removeTypes": "flow-remove-types --pretty src/**/*.js --out-dir dist && ./node_modules/.bin/babel-node ./scripts/stripFlowComments.js",
    "test:e2e": "cypress open",
    "coffee:compile": "./node_modules/.bin/coffee -c ./",
    "coffee:watch": "./node_modules/.bin/coffee -cw ./"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.6",
    "connect-history-api-fallback": "^1.3.0",
    "connected-react-router": "^6.8.0",
    "crypto-js": "^3.1.9-1",
    "d3-scale": "^1.0.5",
    "given2": "^2.1.7",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.2.0",
    "numeral": "^2.0.6",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-reporter": "^4.0.0",
    "postcss-url": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "16.9.0",
    "react-dates": "17.1.1",
    "react-document-title": "^2.0.3",
    "react-dom": "16.9.0",
    "react-moment-proptypes": "^1.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.1.1",
    "react-vis": "1.11.6",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "amazon-cognito-identity-js": "^2.0.3",
    "auditjs": "^2.1.8",
    "aws-sdk": "^2.224.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "basscss": "^8.0.3",
    "basscss-grid": "^2.0.0",
    "browser-sync": "^2.18.12",
    "coffeescript": "^2.5.1",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cypress": "4.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-redux": "^0.1.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.77.0",
    "flow-coverage-report": "^0.3.0",
    "flow-remove-types": "^1.2.1",
    "flow-scripts": "^0.4.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.0.2",
    "jsdom": "11.1.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "json2csv": "^4.3.2",
    "livingcss": "^4.3.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.0.2",
    "nprogress": "^0.2.0",
    "postcss": "^6.0.5",
    "postcss-cli": "^4.1.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "qs": "^6.5.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": ">=0.14",
    "react-dom-factories": "^1.0.0",
    "react-table": "^6.8.6",
    "react-test-renderer": "^16.0.0",
    "react-with-styles": "^2.2.0",
    "react-with-styles-interface-css": "^2.0.2",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.2.3",
    "redux-test-utils": "^0.1.2",
    "reset-css": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.7",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest": {
    "notify": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      ".",
      "src",
      "node_modules"
    ],
    "roots": [
      "tests/unit/"
    ],
    "transformIgnorePatterns": [
      "node_modules/crypto-browserify/rng.js"
    ],
    "coverageDirectory": "./dist/coverage",
    "globals": {
      "__SURE_CONFIG__": {
        "ENV": "test",
        "API_GATEWAY": "https://analytics-api-internal.stable.aws.ean"
      },
      "__PROD__": false
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/utils/mocks.js"
  }
}
